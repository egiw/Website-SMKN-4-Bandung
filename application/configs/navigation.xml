<?xml version="1.0" encoding="UTF-8"?>
<configdata>
    <frontend>
        <home>
            <label>{icon-home} Beranda</label>
            <controller>index</controller>
            <action>index</action>
        </home>
        <profile>
            <label>{icon-info-sign} Profil</label>
            <controller>profile</controller>
            <action>index</action>
        </profile>
        <prakerin>
            <label>{icon-map-marker} Info Prakerin</label>
            <controller>prakerin</controller>
            <action>index</action>
        </prakerin>
        <news>
            <label>{icon-rss} Berita</label>
            <controller>news</controller>
            <action>index</action>
        </news>
        <article>
            <label>{icon-pushpin} Artikel</label>
            <controller>article</controller>
            <action>index</action>
        </article>
        <gallery>
            <label>{icon-picture} Galeri</label>
            <controller>gallery</controller>
            <action>index</action>
        </gallery>
        <guestbook>
            <label>{icon-book} Buku Tamu</label>
            <controller>guestbook</controller>
            <action>index</action>
        </guestbook>

    </frontend>


    <admin>

        <content>
            <label>{icon-folder-close} Konten</label>
            <uri>#collapseOne</uri>
            <pages>
                <article>
                    <resource>article</resource>
                    <privilege>index</privilege>
                    <label>Artikel</label>
                    <module>admin</module>
                    <controller>article</controller>
                    <action>index</action>
                </article>
                <news>
                    <resource>news</resource>
                    <label>Berita</label>
                    <module>admin</module>
                    <controller>news</controller>
                    <action>index</action>
                </news>
                <event>
                    <resource>event</resource>
                    <label>Kegiatan</label>
                    <module>admin</module>
                    <controller>event</controller>
                    <action>index</action>
                </event>
                <gallery>
                    <resource>gallery</resource>
                    <label>Galeri</label>
                    <module>admin</module>
                    <controller>gallery</controller>
                    <action>index</action>
                </gallery>
                <mading>
                    <resource>mading</resource>
                    <label>Majalah Dinding</label>
                    <module>admin</module>
                    <controller>mading</controller>
                    <action>index</action>
                </mading>
                <polling>
                    <resource>polling</resource>
                    <label>Jajak Pendapat</label>
                    <module>admin</module>
                    <controller>polling</controller>
                    <action>index</action>
                </polling>
                <highlight>
                    <resource>highlight</resource>
                    <label>Highlight</label>
                    <module>admin</module>
                    <controller>highlight</controller>
                    <action>index</action>
                </highlight>
                <jobs>
                    <resource>jobs</resource>
                    <label>Lowongan Kerja</label>
                    <module>admin</module>
                    <controller>jobs</controller>
                    <action>index</action>
                </jobs>
                <prakerin>
                    <resource>prakerin</resource>
                    <label>Prakerin</label>
                    <module>admin</module>
                    <controller>prakerin</controller>
                    <action>index</action>
                </prakerin>
            </pages>
        </content>

        <admin>
            <resource>admin</resource>
            <uri>#collapseTwo</uri>
            <label>{icon-lock} Admin</label>
            <pages>
                <article>
                    <label>Setujui Artikel (:articleCount)</label>
                    <module>admin</module>
                    <controller>article</controller>
                    <action>approve</action>
                    <resource>article</resource>
                    <privilege>approve</privilege>
                </article>
                <account>
                    <resource>account</resource>
                    <label>Akun</label>
                    <module>admin</module>
                    <controller>account</controller>
                    <action>index</action>
                </account>
            </pages>
        </admin>

    </admin>

</configdata>